<addons>
  <addon id="plugin.video.alluc.api" name="Alluc" provider-name="DudeHere" version="1.2.10">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.dudehere.routines"/>
    </requires>
    <extension library="default.py" point="xbmc.python.pluginsource">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>I want my All-You-See!</summary>
        <description>Turns kodi into an instant free on-demand media-center. Browse and play movies and series found all over the internet.</description>
        <news>Last Version That Dudehere Did</news>
    </extension>
</addon>
  <addon id="plugin.video.TheDeLorean" version="3.0.3" name="The DeLorean" provider-name="Kodi Revolution">
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
		<import addon="plugin.video.youtube" version="5.4.14Bump" />
		<import addon="script.module.pyxbmct" version="1.2.0" />
		<import addon="script.TheDeLorean.artwork" version="1.0.1" />
		<import addon="script.TheDeLorean.metadata" version="1.0.1" />
		<import addon="script.module.urlresolver" version="4.0.01" />
		<import addon="script.module.liveresolver" version="0.1.49" />
		<import addon="script.module.metahandler" version="3.0.2" />
		<import addon="script.module.TheDeLorean" version="1.0.1" />
		<import addon="script.video.F4mProxy" version="2.8.7"/>
		<import addon="plugin.video.f4mTester" version="2.7.1"/>
		
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.service" library="service.py" start="startup" />
	<extension point="xbmc.addon.metadata">
		<summary lang="en">The DeLorean</summary>
		<description lang="en">Doc Brown really likes it here!</description>
		<platform>all</platform>
		<license>GNU GPL 3</license>
		<forum></forum>
		<website></website>
		<source></source>
		<disclaimer lang="en">The DeLorean does not host any of the content found here.</disclaimer>

		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
    			<screenshot></screenshot>
		</assets>
		<news></news>
	</extension>
</addon>
  </addons>


